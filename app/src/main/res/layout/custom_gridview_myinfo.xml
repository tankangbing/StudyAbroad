<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#fff"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@color/colorPrimary"
        android:gravity="center_vertical" >

        <Button
            android:id="@+id/st"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:layout_marginLeft="16dp"
            android:background="@drawable/back"
            android:onClick="msgdetailBack"
            android:textColor="#fff"
            style="@style/app_copyright" />

        <TextView
            android:id="@+id/msgbartitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/st"
            android:layout_alignBottom="@+id/st"
            android:layout_centerHorizontal="true"
            android:text="我的信息"
            android:textAppearance="?android:attr/textAppearanceLarge"
            style="@style/app_tar"
             />

    </RelativeLayout>

    <GridView
        android:id="@+id/myinfo_gridview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/table_frame_style"
        android:layout_below="@+id/relativeLayout1"
        android:listSelector="#00000000"
		android:horizontalSpacing="50dp"
		android:verticalSpacing="20dp"
        android:layout_margin="10dp"
        android:padding="50dp"
        android:numColumns="2" >

    </GridView>

</RelativeLayout>
